{
  "name": "hatebupwa",
  "version": "1.0.0",
  "description": "Hatena Bookmark search app.",
  "homepage": "https://hatebupwa.netlify.app/",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:worker": "vite build vite.config.worker.js",
    "eject": "react-scripts eject",
    "start": "vite",
    "start:react": "react-scripts start",
    "test": "react-scripts test --env=node",
    "sw:generate": "workbox generateSW workbox-config.js",
    "package": "node tools/package-app.js",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks",
    "dev": "vite"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "dependencies": {
    "@almin/react-context": "^1.1.1",
    "@types/debug": "^0.0.30",
    "@types/history": "^4.6.2",
    "@types/lodash.uniqby": "^4.5.6",
    "@types/prop-types": "^15.5.2",
    "@uifabric/icons": "^7.1.1",
    "almin": "^0.17.1",
    "almin-logger": "^6.2.1",
    "asocial-bookmark": "^1.0.3",
    "comlink": "^4.0.2",
    "date-fns": "^1.29.0",
    "ddd-base": "^0.6.0",
    "debounce-promise": "^3.1.0",
    "debug": "^3.1.0",
    "hatebu-mydata-parser": "^1.0.0",
    "highlight-words-core": "^1.2.0",
    "history": "^4.7.2",
    "localforage": "^1.7.1",
    "localforage-memoryStorageDriver": "^0.9.2",
    "lodash.uniqby": "^4.5.0",
    "micro-memoize": "^2.0.1",
    "office-ui-fabric-react": "^7.19.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-highlight-words": "^0.11.0",
    "react-routing-resolver": "3.0.0",
    "react-scripts": "^3.0.1",
    "regex-combiner": "^1.0.1",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^20.11.5",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.19.1",
    "lint-staged": "^12.1.2",
    "nativefier": "^7.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "workbox-cli": "^3.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.1.1",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "resolutions": {
    "@types/react": "16.3.8",
    "@types/react-dom": "16.0.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "post-commit": "git reset",
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
